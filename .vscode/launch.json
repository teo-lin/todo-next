{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "next:app",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run-script", "dev"],
      "skipFiles": ["<node_internals>/**"],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "restart": true,
      "timeout": 5000,
      "sourceMaps": true,
      "outFiles": []
    }
  ]
}
